<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.roplus.spherecpov">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login.ActivityLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityMain" />
        <activity android:name=".RegisterUser.ActivityRegisterUser" />
        <activity android:name=".BookSession.ActivityBookSession" />
        <activity android:name=".CheckIn.ActivityCheckIn" />
        <activity android:name=".AddSession.ActivityAddSession" />
        <activity android:name=".DeleteSession.ActivityDeleteSession"></activity>
    </application>

</manifest>